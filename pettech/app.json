      "notification": {
        "icon": "./assets/icon.png",  
        "color": "#FF231F7C",  
        "priority": "high", 
        "badge": true  
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "fecd9c3e-737a-4e2a-ac3a-10f398c3435d"
