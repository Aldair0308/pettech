{
  "expo": {
    "name": "pettech", 
    "slug": "pettech",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_pet.jpg",  
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "statusBar": {
        "style": "dark-content",
        "backgroundColor": "#ffffff"
      },
      "notification": {
        "icon": "./assets/icon.png",  
        "color": "#FF231F7C",  
        "displayInForeground": true 
