{
  "expo": {
    "name": "pettech",
    "slug": "pettech",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon_pet.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.radial51.pettech",
      "permissions": ["NOTIFICATIONS"],
      "notification": {
        "icon": "./assets/icon.png",
        "color": "#FF231F7C",
        "priority": "high",
        "badge": true,
        "channelId": "default"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "fecd9c3e-737a-4e2a-ac3a-10f398c3435d"
      }
    },
    "owner": "radial-51",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#FF231F7C",
          "sounds": ["default"]
        }
      ]
    ]
  }
}
